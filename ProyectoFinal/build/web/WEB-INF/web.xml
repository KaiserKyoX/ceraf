<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CreaReserva</servlet-name>
        <servlet-class>Servlet.CreaReserva</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>nuevoEspacio</servlet-name>
        <servlet-class>Servlet.nuevoEspacio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pruebas</servlet-name>
        <servlet-class>Servlet.pruebas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TipoEspacioBD</servlet-name>
        <servlet-class>Controlador.TipoEspacioBD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TipoEspacio</servlet-name>
        <servlet-class>Servlet.TipoEspacio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SelectTipos</servlet-name>
        <servlet-class>Servlet.SelectTipos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>formnuevoespacio</servlet-name>
        <servlet-class>Servlet.formnuevoespacio</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreaReserva</servlet-name>
        <url-pattern>/CreaReserva</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>nuevoEspacio</servlet-name>
        <url-pattern>/nuevoEspacio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pruebas</servlet-name>
        <url-pattern>/pruebas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TipoEspacioBD</servlet-name>
        <url-pattern>/TipoEspacioBD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TipoEspacio</servlet-name>
        <url-pattern>/TipoEspacio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SelectTipos</servlet-name>
        <url-pattern>/SelectTipos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>formnuevoespacio</servlet-name>
        <url-pattern>/formnuevoespacio</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
